function setup() {
  createCanvas(1000,800);
  background("rgb(203,102,204)");
}

function draw() {
  fill("rgb(104,28,112)");
  stroke("#EEE7F0,194)");
  if (mouseIsPressed)
  circle(mouseX,mouseY,50);
}

html, body {
  margin: 0;
  padding: 0;
}
canvas {
  display: block;
}

